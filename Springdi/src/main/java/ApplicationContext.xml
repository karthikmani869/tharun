<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<context:annotation-config></context:annotation-config>

	<bean id="c1" class="com.klu.Springdi.Course">
		<constructor-arg value="1" type="int" />
		<constructor-arg value="jfsd" />
		<constructor-arg value="o" />
	</bean>

	<bean id="c2" class="com.klu.Springdi.Course">
		<constructor-arg value="2" type="int" />
		<constructor-arg value="mswd" />
		<constructor-arg value="a" />
	</bean>

	<bean id="r1" class="com.klu.Springdi.Result">
		<property name="l">
			<list>
				<ref bean="c1" />
				<ref bean="c2" />
			</list>
		</property>

		<property name="s">
			<set>
				<ref bean="c1" />
				<ref bean="c2" />
			</set>
		</property>

		<property name="m">
			<map>
				<entry key="1" value-ref="c1"></entry>
				<entry key="2" value-ref="c2"></entry>
			</map>
		</property>

		<property name="p">
			<props>
				<prop key="1"> property1 </prop>
				<prop key="2"> property2 </prop>
			</props>
		</property>
	</bean>

	<bean id="s1" class="com.klu.Springdi.Student">
		<property name="id" value="1"></property>
		<property name="name" value="Balajee R M"></property>
		<property name="course" ref="c1"></property>
		<!--  <property name="result" ref="r1"></property>-->
	</bean>

</beans>